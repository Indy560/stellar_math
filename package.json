{
  "name": "stellar-math",
  "version": "0.1.0",
  "description": "service template for building scalable APIs",
  "scripts": {
    "start": "node ./src/app/app.js",
    "lint": "./node_modules/.bin/eslint -f tap --ignore-pattern '/src/app/test/*' ./",
    "test": "npm run lint && node node_modules/.bin/istanbul cover --dir ./src/app/test/coverage --report lcov -x 'app.js' -x '**/src/app/util/**' -x '**/src/app/model/**' node_modules/.bin/cucumber.js -- ./src/app/test/",
    "deploy": ""
  },
  "pre-commit": [
    "lint"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/JSVillage/stellar_math.git"
  },
  "author": "JSVillage",
  "dependencies": {
    "body-parser": "1.15.2",
    "compression": "1.6.2",
    "connect-redis": "3.1.0",
    "express": "4.14.0",
    "express-handlebars": "3.0.0",
    "express-session": "1.14.0",
    "pm2": "1.1.3",
    "q": "1.4.1",
    "redis": "2.6.2"
  },
  "devDependencies": {
    "cucumber": "1.2.1",
    "eslint": "3.2.2",
    "eslint-plugin-promise": "2.0.0",
    "istanbul": "0.4.4",
    "pre-commit": "1.1.3"
  }
}
